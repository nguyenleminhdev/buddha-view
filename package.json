{
  "name": "buddha-view",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite --mode dev --open",
    "staging": "vite --mode staging --open",
    "prod": "vite --mode prod --open",
    "build-dev": "run-p type-check build-only-dev",
    "build-staging": "run-p type-check build-only-staging",
    "build-prod": "run-p type-check build-only-prod",
    "build": "run-p type-check build-only-prod",
    "build-only-dev": "vite build --mode dev",
    "build-only-staging": "vite build --mode staging",
    "build-only-prod": "vite build --mode prod",
    "type-check": "vue-tsc --noEmit"
  },
  "dependencies": {
    "async": "^3.2.4",
    "autoprefixer": "^10.4.13",
    "date-fns": "2.30.0",
    "lodash": "^4.17.21",
    "pinia": "^2.0.28",
    "postcss": "^8.4.19",
    "query-string": "^8.1.0",
    "sass": "^1.56.2",
    "sweetalert2": "^11.7.12",
    "tailwindcss": "^3.2.4",
    "vue": "^3.2.45",
    "vue-i18n": "^9.2.2",
    "vue-router": "^4.1.6"
  },
  "devDependencies": {
    "@types/async": "^3.2.16",
    "@types/lodash": "^4.14.191",
    "@types/node": "^18.16.18",
    "@vitejs/plugin-basic-ssl": "^1.0.1",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vue/tsconfig": "^0.1.3",
    "npm-run-all": "^4.1.5",
    "typescript": "~4.7.4",
    "vite": "^4.0.0",
    "vue-tsc": "^1.0.12"
  }
}
